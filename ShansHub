-- Teleport Features
local savedPosition = nil
TeleportSection:AddButton({
    Name = "Save Position",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            savedPosition = player.Character.HumanoidRootPart.CFrame
            OrionLib:MakeNotification({
                Name = "Position Saved",
                Content = "Position saved successfully!",
                Time = 5,
                Icon = "rbxassetid://4483345998"
            })
        end
    end
})
TeleportSection:AddButton({
    Name = "Teleport to Saved Position",
    Callback = function()
        local player = game.Players.LocalPlayer
        if savedPosition and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = savedPosition
            OrionLib:MakeNotification({
                Name = "Teleported",
                Content = "Teleported to saved position!",
                Time = 5,
                Icon = "rbxassetid://4483345998"
            })
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "No saved position found!",
                Time = 5,
                Icon = "rbxassetid://6031071053"
            })
        end
    end
})
TeleportSection:AddToggle({
    Name = "Teleport to Player GUI",
    Callback = function(enabled)
        -- Create GUI elements
        local player = game.Players.LocalPlayer
        local playerGui = player:WaitForChild("PlayerGui")
        
        -- Ensure GUI doesn't duplicate
        local teleportGui = playerGui:FindFirstChild("TeleportGui")
        if not teleportGui then
            teleportGui = Instance.new("ScreenGui")
            teleportGui.Name = "TeleportGui"
            teleportGui.Parent = playerGui

            -- Frame
            local frame = Instance.new("Frame")
            frame.Name = "TeleportFrame"
            frame.Size = UDim2.new(0, 250, 0, 150)
            frame.Position = UDim2.new(0.5, -125, 0.5, -75)
            frame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
            frame.BorderSizePixel = 0
            frame.Parent = teleportGui

            -- Make frame draggable
            local dragging, dragInput, dragStart, startPos

            frame.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = true
                    dragStart = input.Position
                    startPos = frame.Position

                    input.Changed:Connect(function()
                        if input.UserInputState == Enum.UserInputState.End then
                            dragging = false
                        end
                    end)
                end
            end)

            frame.InputChanged:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseMovement then
                    dragInput = input
                end
            end)

            game:GetService("UserInputService").InputChanged:Connect(function(input)
                if input == dragInput and dragging then
                    local delta = input.Position - dragStart
                    frame.Position = UDim2.new(
                        startPos.X.Scale,
                        startPos.X.Offset + delta.X,
                        startPos.Y.Scale,
                        startPos.Y.Offset + delta.Y
                    )
                end
            end)

            -- TextBox
            local textBox = Instance.new("TextBox")
            textBox.Name = "VictimTextBox"
            textBox.Size = UDim2.new(0, 200, 0, 30)
            textBox.Position = UDim2.new(0.5, -100, 0.2, 0)
            textBox.PlaceholderText = "Enter username or UserId"
            textBox.Text = ""
            textBox.TextColor3 = Color3.new(1, 1, 1)
            textBox.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
            textBox.Parent = frame

            -- Button
            local teleportButton = Instance.new("TextButton")
            teleportButton.Name = "TeleportButton"
            teleportButton.Size = UDim2.new(0, 200, 0, 30)
            teleportButton.Position = UDim2.new(0.5, -100, 0.6, 0)
            teleportButton.Text = "Teleport"
            teleportButton.TextColor3 = Color3.new(1, 1, 1)
            teleportButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
            teleportButton.Parent = frame

            -- Teleport logic
            teleportButton.MouseButton1Click:Connect(function()
                local victimNameOrId = textBox.Text
                if victimNameOrId ~= "" then
                    local victim
                    if tonumber(victimNameOrId) then
                        victim = game.Players:GetPlayerByUserId(tonumber(victimNameOrId))
                    else
                        victim = game.Players:FindFirstChild(victimNameOrId)
                    end

                    if victim and victim.Character and victim.Character:FindFirstChild("HumanoidRootPart") then
                        player.Character.HumanoidRootPart.CFrame = victim.Character.HumanoidRootPart.CFrame
                        print("Teleported to " .. victim.Name .. "!")
                        OrionLib:MakeNotification({
                            Name = "Teleported!",
                            Content = "You have teleported to " .. victim.Name .. ".",
                            Time = 5,
                            Icon = "rbxassetid://4483345998"
                        })
                    else
                        print("Victim not found.")
                        OrionLib:MakeNotification({
                            Name = "Error",
                            Content = "Could not find the victim or their character.",
                            Time = 5,
                            Icon = "rbxassetid://6031071053"
                        })
                    end
                else
                    OrionLib:MakeNotification({
                        Name = "Error",
                        Content = "Textbox is empty. Enter a username or UserId.",
                        Time = 5,
                        Icon = "rbxassetid://6031071053"
                    })
                end
            end)
        end

        teleportGui.Enabled = enabled
    end
})
-- Troll feature
TrollSection:AddButton({
    Name = "Teleport on click",
    Callback = function()
        -- Configurations for the teleport feature
        local Config = {
            Bind = Enum.KeyCode.T, -- Key to toggle the teleport mode
            Active_Color = Color3.fromRGB(0, 255, 0), -- Green
            Inactive_Color = Color3.fromRGB(255, 0, 0) -- Red
        }

        -- Services and initial setup
        local UserInputService = game:GetService("UserInputService")
        local Players = game:GetService("Players")
        local Mouse = Players.LocalPlayer:GetMouse()
        local Player = Players.LocalPlayer
        local Char = Player.Character or Player.CharacterAdded:Wait()
        local RootPart = Char:WaitForChild("HumanoidRootPart")

        -- Create Indicator GUI
        local ScreenGui = Instance.new("ScreenGui", Player:WaitForChild("PlayerGui"))
        local IndicatorFrame = Instance.new("Frame", ScreenGui)
        IndicatorFrame.Size = UDim2.new(0, 100, 0, 50)
        IndicatorFrame.Position = UDim2.new(0, 10, 1, -60) -- Bottom left corner
        IndicatorFrame.BackgroundColor3 = Config.Inactive_Color -- Default is red

        local TeleportEnabled = false

        -- Toggle teleport mode
        UserInputService.InputBegan:Connect(function(Input, GPE)
            if GPE then return end
            if Input.KeyCode == Config.Bind then
                TeleportEnabled = not TeleportEnabled
                if TeleportEnabled then
                    IndicatorFrame.BackgroundColor3 = Config.Active_Color -- Green when active
                else
                    IndicatorFrame.BackgroundColor3 = Config.Inactive_Color -- Red when inactive
                end
            end
        end)

        -- Teleport player on mouse click
        Mouse.Button1Down:Connect(function()
            if TeleportEnabled and Mouse.Target then
                local TargetPosition = Mouse.Hit.Position
                RootPart.CFrame = CFrame.new(TargetPosition + Vector3.new(0, 3, 0)) -- Add offset to avoid getting stuck in objects
            end
        end)

        -- Notification to remind users
        OrionLib:MakeNotification({
            Name = "Reminder",
            Content = "This only supports laptop/PC users! Keybind to activate its T",
            Time = 5,
            Icon = "rbxassetid://6031071053"
        })
    end
})

TrollSection:AddButton({
    Name = "Fling on click",
    Callback = function()
        local Config = {
            Bind = Enum.KeyCode.Z, -- Key to toggle fling mode
            Select_Color = Color3.fromRGB(0, 255, 0), -- Selection Box color
            Active_Color = Color3.fromRGB(0, 255, 0), -- Green
            Inactive_Color = Color3.fromRGB(255, 0, 0), -- Red
            Prediction_Enable = true
        }

        local UserInputService = game:GetService("UserInputService")
        local RunService = game:GetService("RunService")
        local Players = game:GetService("Players")

        local Player = Players.LocalPlayer
        local Char = Player.Character or Player.CharacterAdded:Wait()
        local Root = Char:WaitForChild("HumanoidRootPart")
        local Humanoid = Char:WaitForChild("Humanoid")
        local Mouse = Player:GetMouse()

        -- Create Indicator GUI
        local ScreenGui = Instance.new("ScreenGui", Player:WaitForChild("PlayerGui"))
        local IndicatorFrame = Instance.new("Frame", ScreenGui)
        IndicatorFrame.Size = UDim2.new(0, 100, 0, 50)
        IndicatorFrame.Position = UDim2.new(1, -110, 1, -60) -- Bottom right corner
        IndicatorFrame.BackgroundColor3 = Config.Inactive_Color -- Default is red

        local SelectionBox = Instance.new("SelectionBox", script)
        SelectionBox.Color3 = Config.Select_Color
        SelectionBox.LineThickness = 0.02

        local FlingEnabled = false
        local A_2 = false

        -- Toggle fling mode
        UserInputService.InputBegan:Connect(function(Input, GPE)
            if GPE then return end
            if Input.KeyCode == Config.Bind then
                if not A_2 then
                    FlingEnabled = not FlingEnabled
                    SelectionBox.Adornee = nil
                    IndicatorFrame.BackgroundColor3 = FlingEnabled and Config.Active_Color or Config.Inactive_Color
                end
            end
        end)

        -- Highlight target with SelectionBox
        Mouse.Move:Connect(function()
            if FlingEnabled then
                if Mouse.Target then
                    if Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") then
                        SelectionBox.Adornee = Mouse.Target.Parent
                    else
                        SelectionBox.Adornee = nil
                    end
                else
                    SelectionBox.Adornee = nil
                end
            end
        end)

        -- Fling functionality
        Mouse.Button1Down:Connect(function()
            if FlingEnabled and not A_2 then
                local Character = SelectionBox.Adornee

                if Character then
                    A_2 = true

                    local BodyGyro = Instance.new("BodyGyro", Root)
                    BodyGyro.CFrame = CFrame.Angles(math.huge, math.huge, math.huge)

                    local LastPos = Root.CFrame

                    while Root and Character:FindFirstChild("HumanoidRootPart") do
                        RunService.Heartbeat:Wait()

                        if Character.HumanoidRootPart.Velocity.Magnitude <= 100 then
                            if Config.Prediction_Enable then
                                local PredictedPosition = Character.HumanoidRootPart.Position
                                    + (Character.Humanoid.MoveDirection * Character.Humanoid.WalkSpeed) / 2
                                    + Vector3.new(0, Character.HumanoidRootPart.Velocity.Y / 10, 0)

                                Root.CFrame = CFrame.new(PredictedPosition) * Root.CFrame.Rotation
                            else
                                Root.CFrame = Character.HumanoidRootPart.CFrame * Root.CFrame.Rotation
                            end

                            Root.Velocity = Vector3.new()
                        else
                            break
                        end
                    end

                    BodyGyro:Destroy()
                    Root.AssemblyLinearVelocity = Vector3.new()
                    Root.AssemblyAngularVelocity = Vector3.new()
                    Root.CFrame = LastPos

                    Humanoid:ChangeState(Enum.HumanoidStateType.Landed)
                end

                A_2 = false
            end
        end)

        -- Notification
        OrionLib:MakeNotification({
            Name = "Reminder",
            Content = "This only supports laptop/PC users! Keybind to activate is Z",
            Time = 5,
            Icon = "rbxassetid://6031071053"
        })
    end
})
TrollSection:AddButton({
    Name = "Ultra Instinct",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        local userInputService = game:GetService("UserInputService")
        local ultraInstinctEnabled = false
        local detectionRadius = 25
        local detectionLoop

        -- Create a frame for the visual indicator
        local screenGui = Instance.new("ScreenGui", player.PlayerGui)
        local statusFrame = Instance.new("Frame", screenGui)
        statusFrame.Size = UDim2.new(0, 100, 0, 50)
        statusFrame.Position = UDim2.new(0, 10, 0, 10)
        statusFrame.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red for disabled

        -- Function to teleport the player
        local function teleportPlayer()
            local directions = {
                Vector3.new(-10, 0, 0), -- Left
                Vector3.new(10, 0, 0), -- Right
                Vector3.new(0, 0, -10), -- Backward
                Vector3.new(0, 0, 10) -- Forward
            }
            local randomDirection = directions[math.random(1, #directions)]
            local newPosition = humanoidRootPart.Position + randomDirection
            humanoidRootPart.CFrame = CFrame.new(newPosition)
        end

        -- Detection logic
        local function detectNearbyPlayers()
            for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    local otherRootPart = otherPlayer.Character.HumanoidRootPart
                    if (humanoidRootPart.Position - otherRootPart.Position).Magnitude <= detectionRadius then
                        teleportPlayer()
                        break
                    end
                end
            end
        end

        -- Toggle Ultra Instinct
        local function toggleUltraInstinct()
            ultraInstinctEnabled = not ultraInstinctEnabled
            if ultraInstinctEnabled then
                print("Ultra Instinct Enabled")
                statusFrame.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green for enabled
                detectionLoop = task.spawn(function()
                    while ultraInstinctEnabled do
                        detectNearbyPlayers()
                        task.wait(0.1)
                    end
                end)
            else
                print("Ultra Instinct Disabled")
                statusFrame.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red for disabled
                if detectionLoop then
                    task.cancel(detectionLoop)
                end
            end
        end

        -- Detect key press
        userInputService.InputBegan:Connect(function(input, gameProcessed)
            if gameProcessed then return end
            if input.KeyCode == Enum.KeyCode.U then
                toggleUltraInstinct()
            end
        end)

        -- Notification
        OrionLib:MakeNotification({
            Name = "Reminder",
            Content = "This only supports laptop/PC users! Keybind to activate is U",
            Time = 5,
            Icon = "rbxassetid://6031071053"
        })
    end
})

-- Fun Features
FunSection:AddButton({
    Name = "Explosive Jump",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            -- Apply an upward force to simulate an explosive jump
            local humanoidRootPart = player.Character.HumanoidRootPart
            local bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.MaxForce = Vector3.new(50000, 50000, 50000)  -- Strong force in all directions
            bodyVelocity.Velocity = Vector3.new(0, 100, 0)  -- Propel upwards
            bodyVelocity.Parent = humanoidRootPart

            -- Make sure to remove the BodyVelocity after a short time to prevent continuous force application
            game:GetService("Debris"):AddItem(bodyVelocity, 0.2)  -- Clean up after 0.2 seconds

            -- Notify the user of the explosive jump
            OrionLib:MakeNotification({
                Name = "BOOM!",
                Content = "You performed an explosive jump!",
                Time = 5,
                Icon = "rbxassetid://4483345998"
            })
        end
    end
})
FunSection:AddButton({
    Name = "Sparkling Effect",
    Callback = function()
        -- Get the player and their character
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local torso = character:WaitForChild("HumanoidRootPart") -- You can use any part like "Torso" or "Head"

        -- Create sparkles
        local sparkles = Instance.new("Sparkles")
        sparkles.Color = Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255)) -- Random color
        sparkles.Parent = torso -- Attach sparkles to the part

        -- Notify the user that the effect is applied
        OrionLib:MakeNotification({
            Name = "Sparkling Effect",
            Content = "Not working :(",
            Time = 5,
            Icon = "rbxassetid://6031071053"
        })
    end
})

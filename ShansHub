FunSection:AddButton({
    Name = "Throw Fireball",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        -- Create the fireball
        local fireball = Instance.new("Part")
        fireball.Size = Vector3.new(2, 2, 2)
        fireball.Shape = Enum.PartType.Ball
        fireball.Position = humanoidRootPart.Position + humanoidRootPart.CFrame.LookVector * 5
        fireball.Anchored = false
        fireball.CanCollide = false
        fireball.Material = Enum.Material.Neon
        fireball.Color = Color3.fromRGB(255, 0, 0)  -- Red color
        fireball.Parent = workspace

        -- Add velocity to the fireball
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
        bodyVelocity.Velocity = humanoidRootPart.CFrame.LookVector * 100
        bodyVelocity.Parent = fireball

        -- Destroy the fireball after 5 seconds
        game:GetService("Debris"):AddItem(fireball, 5)

        OrionLib:MakeNotification({
            Name = "Fireball Shot",
            Content = "You threw a fireball!",
            Time = 5,
            Icon = "rbxassetid://6031071053"
        })
    end
})



-- Player Enhancements
PlayerFeature:AddSlider({
    Name = "Speed",
    Min = 16,
    Max = 500,  -- Maximum speed, adjust as needed
    Default = 16,  -- Default Roblox walk speed
    Color = Color3.fromRGB(169, 169, 169),
    Increment = 1,
    ValueName = "Speed",
    Callback = function(Value)
        -- Access the player's humanoid and change the walk speed
        local Player = game.Players.LocalPlayer
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local Humanoid = Character:WaitForChild("Humanoid")

        -- Update the humanoid's walk speed based on the slider value
        Humanoid.WalkSpeed = Value
    end
})
PlayerFeature:AddSlider({
    Name = "Jump Power",
    Min = 50,
    Max = 200,  -- Maximum jump power, adjust as needed
    Default = 50,  -- Default jump power
    Color = Color3.fromRGB(169, 169, 169),
    Increment = 1,
    ValueName = "Jump Power",
    Callback = function(Value)
        -- Access the player's humanoid and change the jump power
        local Player = game.Players.LocalPlayer
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local Humanoid = Character:WaitForChild("Humanoid")

        -- Update the humanoid's jump power based on the slider value
        Humanoid.JumpPower = Value
    end
})
PlayerFeature:AddSlider({
    Name = "Gravity",
    Min = 196.2, -- Minimum gravity (higher value makes it heavier)
    Max = 500, -- Maximum gravity
    Default = 196.2, -- Default gravity
    Color = Color3.fromRGB(169, 169, 169), -- Blue color
    Increment = 1, -- Value will increment by 1 each time
    ValueName = "Gravity",
    Callback = function(Value)
        game.Workspace.Gravity = Value
    end    
})

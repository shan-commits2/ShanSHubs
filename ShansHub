-- Main Features
MainSection:AddToggle({
    Name = "Forcefield Toggle",
    Default = false,
    Callback = function(Value)
        local player = game.Players.LocalPlayer
        local character = player.Character

        if character then
            if Value then
                -- Create a forcefield if the toggle is on
                local forceField = Instance.new("ForceField")
                forceField.Parent = character

                OrionLib:MakeNotification({
                    Name = "Forcefield Enabled",
                    Content = "A forcefield has been enabled around you.",
                    Time = 2,
                    Icon = "rbxassetid://6031071053"
                })
            else
                -- Remove the forcefield if the toggle is off
                for _, child in pairs(character:GetChildren()) do
                    if child:IsA("ForceField") then
                        child:Destroy()
                    end
                end

                OrionLib:MakeNotification({
                    Name = "Forcefield Disabled",
                    Content = "The forcefield has been disabled.",
                    Time = 2,
                    Icon = "rbxassetid://6031071053"
                })
            end
        end
    end
})


MainSection:AddToggle({
    Name = "Freeze Player",
    Default = false,
    Callback = function(Value)
        if Value then
            -- Freeze the player
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character and character:FindFirstChild("Humanoid") then
                -- Set the humanoid's HipWidth and HipHeight to 0 to prevent movement
                character.Humanoid.WalkSpeed = 0
                character.Humanoid.PlatformStand = true

                OrionLib:MakeNotification({
                    Name = "Player Frozen",
                    Content = "Player has been frozen and can no longer move.",
                    Time = 2,
                    Icon = "rbxassetid://6031071053"
                })
            end
        else
            -- Unfreeze the player
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character and character:FindFirstChild("Humanoid") then
                -- Reset humanoid properties to allow movement again
                character.Humanoid.WalkSpeed = 16  -- Default WalkSpeed
                character.Humanoid.PlatformStand = false

                OrionLib:MakeNotification({
                    Name = "Player Unfrozen",
                    Content = "Player has been unfrozen and can now move.",
                    Time = 2,
                    Icon = "rbxassetid://6031071053"
                })
            end
        end
    end
})
MainSection:AddButton({
    Name = "Fly",
    Callback = function()
local main = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local up = Instance.new("TextButton")

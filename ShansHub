----Misc Main
MiscMainSection:AddToggle({
    Name = "Anti-20 Minute Disconnect",
    Default = false,
    Callback = function(Value)
        local player = game.Players.LocalPlayer
        local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
        
        -- Stop any existing loop
        if _G.AntiDisconnectTimer then
            _G.AntiDisconnectTimer:Disconnect()
            _G.AntiDisconnectTimer = nil
        end
        
        -- Start the loop if toggled on
        if Value then
            _G.AntiDisconnectTimer = game:GetService("RunService").RenderStepped:Connect(function()
                if os.clock() % (19 * 60) < 1 then -- Every 19 minutes
                    if humanoid then
                        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    end
                end
            end)
        end
    end
})
-- performance Features
FPSBoostSection:AddButton({
    Name = "FPS Booster",
    Callback = function()
        for _, part in ipairs(workspace:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Material = Enum.Material.SmoothPlastic
                part.CastShadow = false
            end
        end
        local lighting = game:GetService("Lighting")
        lighting.GlobalShadows = false
        lighting.Brightness = 1
        OrionLib:MakeNotification({
            Name = "FPS Boost Activated",
            Content = "Game optimized for better performance!",
            Time = 5,
            Icon = "rbxassetid://4483345998"
        })
    end
})
---misc feature
MiscMainSection:AddButton({
    Name = "Rejoin Server",
    Callback = function()
        -- Function to rejoin the current server
        local TeleportService = game:GetService("TeleportService")
        local Players = game:GetService("Players")
        local player = Players.LocalPlayer

        -- Rejoin the same server
        TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, player)
    end
})
--------beta section






---ESP SECTION
EspSection:AddToggle({
    Name = "Highlight Players and Display Info(wip)",
    Default = false,
    Callback = function(isToggled)
        local Players = game:GetService("Players")
        local UserInputService = game:GetService("UserInputService")

        -- Function to create the health bar, username, and device info above players' heads
        local function createPlayerInfo(player)
            local character = player.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                -- Highlight the player in red except for the local player
                if player ~= game.Players.LocalPlayer then
                    local highlight = Instance.new("Highlight")
                    highlight.Adornee = character
                    highlight.FillColor = Color3.fromRGB(255, 0, 0)
                    highlight.Parent = character
                end

                -- Create a BillboardGui to hold all player information
                local infoGui = Instance.new("BillboardGui")
                infoGui.Adornee = character.Head
                infoGui.Size = UDim2.new(0, 200, 0, 100)
                infoGui.StudsOffset = Vector3.new(0, 3, 0)
                infoGui.Parent = character.Head

                -- Device Type Text (Top)
                local deviceTypeText = Instance.new("TextLabel")
                deviceTypeText.Text = "Device: UNKNOWN"
                deviceTypeText.Size = UDim2.new(1, 0, 0.3, 0)
                deviceTypeText.Position = UDim2.new(0, 0, 0, 0) -- Top position
                deviceTypeText.BackgroundTransparency = 1
                deviceTypeText.TextColor3 = Color3.fromRGB(255, 255, 255)
                deviceTypeText.TextScaled = true
                deviceTypeText.Parent = infoGui

                -- Username Text (Middle)
                local usernameText = Instance.new("TextLabel")
                usernameText.Text = "Username: " .. player.Name
                usernameText.Size = UDim2.new(1, 0, 0.3, 0)
                usernameText.Position = UDim2.new(0, 0, 0.3, 0) -- Middle position
                usernameText.BackgroundTransparency = 1
                usernameText.TextColor3 = Color3.fromRGB(255, 255, 255)
                usernameText.TextScaled = true
                usernameText.Parent = infoGui

                -- Health Text (Bottom)
                local healthText = Instance.new("TextLabel")
                healthText.Text = "Health: " .. math.floor(character:WaitForChild("Humanoid").Health)
                healthText.Size = UDim2.new(1, 0, 0.3, 0)
                healthText.Position = UDim2.new(0, 0, 0.6, 0) -- Bottom position
                healthText.BackgroundTransparency = 1
                healthText.TextColor3 = Color3.fromRGB(255, 255, 255)
                healthText.TextScaled = true
                healthText.Parent = infoGui

                -- Update health in real-time
                local humanoid = character:WaitForChild("Humanoid")
                humanoid.HealthChanged:Connect(function()
                    healthText.Text = "Health: " .. math.floor(humanoid.Health)
                end)
            end
        end

        -- Toggle to show or hide player information
        if isToggled then
            -- Iterate through all players and create info for them
            for _, player in pairs(Players:GetPlayers()) do
                createPlayerInfo(player)
            end

            -- Update player info when players join
            Players.PlayerAdded:Connect(createPlayerInfo)
        else
            -- Clear any existing player info when toggled off
            for _, player in pairs(Players:GetPlayers()) do
                local character = player.Character
                if character and character:FindFirstChild("Head") then
                    local billboardGui = character.Head:FindFirstChildOfClass("BillboardGui")
                    if billboardGui then
                        billboardGui:Destroy()
                    end
                end
            end
        end
    end
})
-- ESP Toggle in EspSection
EspSection:AddToggle({
    Name = "ESP Toggle",  -- Name of the toggle
    Default = false,  -- Default value (ESP off)
    Callback = function(Value)
        if Value then
            -- Enable ESP (You can customize the ESP logic as needed)
            local player = game.Players.LocalPlayer
            for _, obj in pairs(workspace:GetChildren()) do
                if obj:IsA("Model") and obj:FindFirstChild("Head") then
                    local espBox = Instance.new("BillboardGui")
                    espBox.Adornee = obj:FindFirstChild("Head")
                    espBox.Size = UDim2.new(0, 200, 0, 50)
                    espBox.StudsOffset = Vector3.new(0, 2, 0)
                    espBox.Parent = player.PlayerGui
                    local textLabel = Instance.new("TextLabel")
                    textLabel.Size = UDim2.new(1, 0, 1, 0)
                    textLabel.BackgroundTransparency = 1
                    textLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                    textLabel.Text = obj.Name
                    textLabel.Parent = espBox
                end
            end
        else
            -- Disable ESP (Remove existing ESPs)
            for _, gui in pairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
                if gui:IsA("BillboardGui") then
                    gui:Destroy()
                end
            end
        end
    end
})


        OrionLib:MakeNotification({
            Name = "Script Executed",
            Content = "Speed Hub X script has been executed successfully!",
            Time = 5,
            Icon = "rbxassetid://6031071053" -- You can change this icon if you prefer
        })
    end
})
MainHubSection:AddButton({ 
    Name = "Sky Wars", 
    Callback = function()
        -- Execute the script from the provided URL
loadstring(game:HttpGet("https://raw.githubusercontent.com/SkibidiCen/MainMenu/main/Code"))()
        
